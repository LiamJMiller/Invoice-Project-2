ess/lib/eta.js","original":{"line":69,"column":9}},{"generated":{"line":53,"column":2},"source":"../node_modules/cli-progress/lib/eta.js","original":{"line":70,"column":5}},{"generated":{"line":55,"column":0},"source":"../node_modules/cli-progress/lib/eta.js","original":{"line":3,"column":9}},{"generated":{"line":57,"column":0},"source":"../node_modules/cli-progress/lib/eta.js","original":{"line":73,"column":0},"name":"module"},{"generated":{"line":57,"column":6},"source":"../node_modules/cli-progress/lib/eta.js","original":{"line":73,"column":6}},{"generated":{"line":57,"column":7},"source":"../node_modules/cli-progress/lib/eta.js","original":{"line":73,"column":7},"name":"exports"},{"generated":{"line":57,"column":14},"source":"../node_modules/cli-progress/lib/eta.js","original":{"line":73,"column":0}},{"generated":{"line":57,"column":17},"source":"../node_modules/cli-progress/lib/eta.js","original":{"line":73,"column":17},"name":"ETA"},{"generated":{"line":57,"column":20},"source":"../node_modules/cli-progress/lib/eta.js","original":{"line":73,"column":0}}],"sources":{"../node_modules/cli-progress/lib/eta.js":"\n// ETA calculation\nclass ETA{\n\n    constructor(length, initTime, initValue){\n        // size of eta buffer\n        this.etaBufferLength = length || 100;\n\n        // eta buffer with initial values\n        this.valueBuffer = [initValue];\n        this.timeBuffer = [initTime];\n\n        // eta time value\n        this.eta = '0';\n    }\n\n    // add new values to calculation buffer\n    update(time, value, total){\n        this.valueBuffer.push(value);\n        this.timeBuffer.push(time);\n\n        // trigger recalculation\n        this.calculate(total-value);\n    }\n\n    // fetch estimated time\n    getTime(){\n        return this.eta;\n    }\n\n    // eta calculation - request number of remaining events\n    calculate(remaining){\n        // get number of samples in eta buffer\n        const currentBufferSize = this.valueBuffer.length;\n        const buffer = Math.min(this.etaBufferLength, currentBufferSize);\n\n        const v_diff = this.valueBuffer[currentBufferSize - 1] - this.valueBuffer[currentBufferSize - buffer];\n        const t_diff = this.timeBuffer[currentBufferSize - 1] - this.timeBuffer[currentBufferSize - buffer];\n\n        // get progress per ms\n        const vt_rate = v_diff/t_diff;\n\n        // strip past elements\n        this.valueBuffer = this.valueBuffer.slice(-this.etaBufferLength);\n        this.timeBuffer  = this.timeBuffer.slice(-this.etaBufferLength);\n\n        // eq: vt_rate *x = total\n        const eta = Math.ceil(remaining/vt_rate/1000);\n\n        // check values\n        if (isNaN(eta)){\n            this.eta = 'NULL';\n\n        // +/- Infinity --- NaN already handled\n        }else if (!isFinite(eta)){\n            this.eta = 'INF';\n\n        // > 10M s ? - set upper display limit ~115days (1e7/60/60/24)\n        }else if (eta > 1e7){\n            this.eta = 'INF';\n\n        // negative ?\n        }else if (eta < 0){\n            this.eta = 0;\n\n        }else{\n            // assign\n            this.eta = eta;\n        }\n    }\n}\n\nmodule.exports = ETA;"},"lineCount":null}},"error":null,"hash":"82bdafb099e8466644626d6bbecfe471","cacheData":{"env":{}}}